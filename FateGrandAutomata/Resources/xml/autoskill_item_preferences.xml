<?xml version="1.0" encoding="utf-8" ?>
<PreferenceScreen
  xmlns:app="http://schemas.android.com/apk/res-auto">

  <EditTextPreference
    app:key="@string/pref_autoskill_name"
    app:title="Name"
    app:useSimpleSummaryProvider="true"
    app:defaultValue="--"/>

  <EditTextPreference
    app:key="@string/pref_autoskill_cmd"
    app:title="Skill Command"
    app:icon="@drawable/ic_terminal"
    app:useSimpleSummaryProvider="true"
    app:defaultValue=""/>

  <EditTextPreference
    app:key="@string/pref_autoskill_servant"
    app:title="Preferred Servants"
    app:icon="@drawable/ic_crown"
    app:useSimpleSummaryProvider="true"
    app:defaultValue=""/>

  <EditTextPreference
    app:key="@string/pref_autoskill_ce"
    app:title="Preferred CEs"
    app:icon="@drawable/ic_card"
    app:useSimpleSummaryProvider="true"
    app:defaultValue=""/>

  <Preference
    app:key="@string/pref_autoskill_delete"
    app:title="Delete Configuration"
    app:icon="@drawable/ic_delete"/>

</PreferenceScreen>