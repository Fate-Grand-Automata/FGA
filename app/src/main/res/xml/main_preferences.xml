<?xml version="1.0" encoding="utf-8" ?>
<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <Preference
        app:icon="@drawable/ic_apple"
        app:key="@string/pref_nav_refill"
        app:summary="@string/p_refill_summary"
        app:title="@string/p_refill" />

    <Preference
        app:icon="@drawable/ic_formation"
        app:key="@string/pref_nav_battle_config"
        app:summary="@string/p_battle_config_summary"
        app:title="@string/p_battle_config" />

    <Preference
        app:icon="@drawable/ic_troubleshooting"
        app:title="@string/p_nav_troubleshoot">
        <intent
            android:action="android.intent.action.VIEW"
            android:data="@string/link_troubleshoot" />
    </Preference>

    <Preference
        app:icon="@drawable/ic_dots_horizontal"
        app:key="@string/pref_nav_more"
        app:title="@string/p_more_options" />
</PreferenceScreen>